        <tr>
            <th style="background-color: green; color: white; padding: 8px; text-align: center;">No</th>
            <th style="background-color: green; color: white; padding: 8px; text-align: center;">City</th>
            <th style="background-color: green; color: white; padding: 8px; text-align: center;">Action</th>
        </tr>
    </thead>
    <tbody style="background-color: lightgray;">
         {% for city in cities %}
        <tr>
                <td style="text-align: center; color: brown;"><h3>{{ forloop.counter }}</h3></td>
            <td>
                <form action="#" method="POST">
                    {% csrf_token %}
                    <input type="text" name="depcity" value="{{ city.depcity }}" readonly 
                        style="height: 50px;  margin-left:50px; width: calc(100% - 20px); width: 100px; text-align: center; color: blue; border-radius: 5px; border: 1px solid #ccc; font-weight: bold; margin: 5px; padding: 5px;">
</td>
