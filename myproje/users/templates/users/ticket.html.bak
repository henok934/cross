<div class="form-row">
<div style="display: flex; align-items: center; margin-left: 200px;">
    <div style="margin-right: 10px; white-space: nowrap;">First Name:</div>
    <input type="text" id="firstname" name="firstname" maxlength="15" class="form-control" style="width: 200px; text-align: center; text-transform: capitalize;" oninput="validatedepcity(this)" required>
    <span id="error-message" style="color: red; display: none; position: absolute; top: 100%; left: 0;"></span>
</div>
<script>
function validatedepcity(input) {
    var value = input.value.trim();
    var errorMessage = document.getElementById("error-message");
    if (!value.match(/^[a-zA-Z]+$/)) {
        errorMessage.textContent = "Please enter a character for city.";
        errorMessage.style.display = "block";
        input.setCustomValidity("Please enter a character for city.");
    } else if (value.length < 4) {
        errorMessage.textContent = "Please enter at least 4 characters.";
        input.setCustomValidity("Please enter at least 4 characters.");
        errorMessage.style.display = "block";
    } else {
        errorMessage.style.display = "none";
        input.setCustomValidity("");
    }
    input.value = value.charAt(0).toUpperCase() + value.slice(1);
}
</script>
<div style="display: flex; align-items: center; margin-left: 200px;">
   <div style="margin-right: 10px; white-space: nowrap;">Last Name:</div>
    <input type="text" id="lastname" name="lastname" maxlength="15" class="form-control" style="width: 200px; text-align: center; text-transform: capitalize;" oninput="validatedepcity(this)" required>
    <span id="error-message" style="color: red; display: none; position: absolute; top: 100%; left: 0;"></span>
</div>
<script>
function validatedepcity(input) {
    var value = input.value.trim();
    var errorMessage = document.getElementById("error-message");
    if (!value.match(/^[a-zA-Z]+$/)) {
        errorMessage.textContent = "Please enter a character for name.";
        errorMessage.style.display = "block";
        input.setCustomValidity("Please enter a character for name.");
    } else if (value.length < 4) {
        errorMessage.textContent = "Please enter at least 4 characters.";
        input.setCustomValidity("Please enter at least 4 characters.");
        errorMessage.style.display = "block";
    } else {
        errorMessage.style.display = "none";
        input.setCustomValidity("");
    }
    input.value = value.charAt(0).toUpperCase() + value.slice(1);
}
</script>

<div style="display: flex; align-items: center; margin-left: 200px;">
    <div>Phone:</div>
    <input type="text" id="phone" name="phone" maxlength="10" class="form-control" style="width: 200px; text-align: center;" oninput="validatePhone(this)" required>
    <div id="phoneError" style="color: red;"></div>
</div>
<script>
function validatePhone(input) {
    var phoneError = document.getElementById("phoneError");
    var phoneNumber = input.value.replace(/[^0-9]/g, '');
    if (isNaN(parseInt(phoneNumber))) {
        phoneError.textContent = "Please enter a number for phone number.";
        input.setCustomValidity("Please enter a number.");
    } else if (!phoneNumber.startsWith("09")) {
        phoneError.textContent = "Please start with 09.";
        input.setCustomValidity("Please start with 09.");
    } else if (phoneNumber.length !== 10) {
        phoneError.textContent = "Please enter 10 digits only.";
        input.setCustomValidity("Please enter 10 digits only.");
    } else {
        phoneError.textContent = "";
        input.setCustomValidity("");
    }
}
</script>

