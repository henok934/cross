<!--
<select name="seat_no" id="seat_no" class="form-control" style="width:300px; color: green; text-align: center;" required onchange="showSeats();">
    <option value="" disabled selected>Select a seat</option>
    {% for seat in all_seats %}
        <option value="{{ seat }}" {% if seat in booked_seats %}disabled{% endif %}>{{ seat }}</option>
    {% endfor %}
</select>

<div id="seat-display" class="seat-container" style="display: none; margin-top: 20px;">
    {% for seat in all_seats %}
        <div class="seat {% if seat in booked_seats %}booked{% endif %}"
             id="seat-{{ seat }}"
             onclick="selectSeat('{{ seat }}');"
             {% if seat in booked_seats %} title="Booked"{% endif %}>
            {{ seat }}
        </div>
    {% endfor %}
</div>

<div id="seat-message" style="color: blue; display: none;"></div>
<input type="hidden" name="no_seat" id="wenberkutr" value="" maxlength="10" class="center-input" readonly style="width: 120px;">

<style>
    .seat-container {
        display: grid;
        grid-template-columns: repeat(5, 1fr);
        gap: 20px;
        max-width: 500px;
        margin: 20px auto;
    }
    .seat {
        width: 40px;
        height: 40px;
        background-color: green; /* Unbooked color */
        color: white;
        display: flex;
        align-items: center;
        justify-content: center;
        border-radius: 50%;
        cursor: pointer;
        transition: background-color 0.3s;
        font-size: 14px;
    }
    .booked {
        background-color: red; /* Booked color */
        cursor: not-allowed;
    }
    .selected {
        background-color: yellow; /* Highlight selected seat */
    }
</style>

<script>
    function showSeats() {
        const seatDisplay = document.getElementById('seat-display');
        seatDisplay.style.display = 'block'; // Show the seat display
    }

    function selectSeat(seat) {
        const wenberkutrInput = document.getElementById('wenberkutr');
        const seatMessage = document.getElementById('seat-message');
        const selectedSeat = document.querySelector('.selected');

        // Reset previously selected seat
        if (selectedSeat) {
            selectedSeat.classList.remove('selected');
        }

        // Check if the seat is booked
        if (document.querySelector(`#seat-${seat}.booked`)) {
            seatMessage.textContent = "This seat is booked.";
            seatMessage.style.display = 'block';
            return;
        }

        wenberkutrInput.value = seat; // Set the hidden input value
        seatMessage.textContent = `Selected Seat: ${seat}`;
        seatMessage.style.display = 'block';

        // Highlight selected seat
        const seatElement = document.getElementById(`seat-${seat}`);
        seatElement.classList.add('selected');
    }
</script>
