<table style="margin: 20px auto; width: 100%; max-width: 600px; background-color: lightgray; border-collapse: collapse; font-size: 16px; margin-top: 50px;">
    <thead>
        <tr style="background-color: green;">
            <th style="padding: 10px;">
                <span style="color: red; font-weight: bold; font-size: 14px;">No</span>
            </th>
            <th style="padding: 10px;">
                <span style="color: white; font-weight: bold; font-size: 14px;">Departure</span>
            </th>
            <th style="padding: 10px;">
                <span style="color: white; font-weight: bold; font-size: 14px;">Destination</span>
            </th>
            <th style="padding: 10px;">
                <span style="color: white; font-weight: bold; font-size: 14px;">Plate No</span>
            </th>
            <th style="padding: 10px;">
                <span style="color: white; font-weight: bold; font-size: 14px;">Travel Date</span>
            </th>
            <th style="padding: 10px;">
                <span style="color: white; font-weight: bold; font-size: 14px;">Remaining Seats</span>
            </th>
            <th style="padding: 10px;">
                <span style="color: white; font-weight: bold; font-size: 14px;">Action</span>
            </th>
        </tr>
    </thead>
    <tbody style="background-color: lightgray;">
        {% for item in routes %}
        <tr style="background-color: {% if forloop.counter|divisibleby:2 %}#f2f2f2{% endif %};">
            <td style="padding: 10px; text-align: center;">
                <span style="color: brown; font-weight: bold; font-size: 14px;">{{ forloop.counter }}</span>
            </td>
            <td style="padding: 10px; text-align: center;">
                <span style="color: blue; font-weight: bold; font-size: 14px;">{{ item.route.depcity }}</span>
            </td>
            <td style="padding: 10px; text-align: center;">
                <span style="color: blue; font-weight: bold; font-size: 14px;">{{ item.route.descity }}</span>
            </td>
            <td style="padding: 10px; text-align: center;">
                <span style="color: blue; font-weight: bold; font-size: 14px;">{{ item.route.plate_no }}</span>
            </td>
            <td style="padding: 10px; text-align: center;">
                <span style="color: blue; font-weight: bold; font-size: 14px;">{{ item.route.date }}</span>
            </td>
            <td style="padding: 10px; text-align: center;">
                <span style="color: red; font-weight: bold; font-size: 14px;">{{ item.remaining_seats }}</span>
            </td>
            <td style="padding: 10px; text-align: center;">
                <form action="{% url 'Select' %}" method="POST" style="margin: 0;">
                    {% csrf_token %}
                    <input type="hidden" name="depcity" value="{{ item.route.depcity }}">
                    <input type="hidden" name="descity" value="{{ item.route.descity }}">
                    <input type="hidden" name="plate_no" value="{{ item.route.plate_no }}">
                    <input type="hidden" name="date" value="{{ item.route.date }}">
                    <button type="submit" style="height: 30px; width: 80px;
                        background-color: {% if item.remaining_seats == 'Full' %}gray{% else %}blue{% endif %};
                        color: brown; border-radius: 5px; border: 1px solid #ccc; font-weight: bold;"
                        {% if item.remaining_seats == 'Full' %}disabled{% endif %}>
                        <span style="color: pink; font-weight: bold; font-size: 14px;">Select</span>
                    </button>
                </form>
            </td>
        </tr>
        {% endfor %}
    </tbody>
</table>








